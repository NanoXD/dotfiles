"*":
  "autocomplete-plus":
    backspaceTriggersAutocomplete: true
    minimumWordLength: 2
  autoprefixer:
    browsers: [
      "last 2 versions"
    ]
  core:
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".flowconfig"
      ]
      "source.json": [
        ".arcconfig"
        "BUCK.autodeps"
      ]
      "source.perl": [
        "cgi"
      ]
      "source.python": [
        "BUCK"
      ]
      "source.ruby": [
        "Dangerfile"
      ]
    disabledPackages: [
      "nuclide"
      "emmet"
    ]
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: []
    themes: [
      "native-ui"
      "deep-space-syntax"
    ]
  editor:
    fontFamily: "Operator Mono"
    invisibles:
      cr: "_"
      tab: "â–¸"
    preferredLineLength: 120
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    softWrapAtPreferredLineLength: true
  "exception-reporting":
    userId: "ec2062ef-1da8-c968-02d8-8039af46975a"
  "file-icons": {}
  "file-types":
    "*.cgi": "source.perl"
  "file-watcher":
    promptWhenFileHasChangedOnDisk: false
  "find-and-replace":
    closeFindPanelAfterSearch: true
    openProjectFindResultsInRightPane: true
  "git-plus":
    pullBeforePush: "pull --rebase"
  linter: {}
  "markdown-writer":
    siteEngine: "jekyll"
  minimap:
    plugins:
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      pigments: true
      pigmentsDecorationsZIndex: 0
  "seti-ui":
    compactView: true
    displayIgnored: false
    font: "Default Atom Font"
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    syncHorizontalScroll: true
  "symbol-gen": {}
  "terminal-plus":
    ansiColors:
      normal:
        black:
          red: 65
          green: 77
          blue: 96
          alpha: 1
        blue:
          red: 96
          green: 140
          blue: 195
          alpha: 1
        cyan:
          red: 86
          green: 173
          blue: 183
          alpha: 1
        green:
          red: 112
          green: 157
          blue: 108
          alpha: 1
        magenta:
          red: 143
          green: 114
          blue: 191
          alpha: 1
        red:
          red: 177
          green: 94
          blue: 124
          alpha: 1
        white:
          red: 117
          green: 135
          blue: 166
          alpha: 1
        yellow:
          red: 181
          green: 162
          blue: 98
          alpha: 1
      zBright:
        brightBlack:
          red: 81
          green: 97
          blue: 125
          alpha: 1
        brightBlue:
          red: 127
          green: 164
          blue: 209
          alpha: 1
        brightCyan:
          red: 122
          green: 190
          blue: 198
          alpha: 1
        brightGreen:
          red: 142
          green: 177
          blue: 139
          alpha: 1
        brightMagenta:
          red: 171
          green: 149
          blue: 207
          alpha: 1
        brightRed:
          red: 194
